{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/contact.js": {
      "runtime": "@vercel/node@2"
    }
  },
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ],
  "routes": [
    { "src": "/contact", "dest": "/components/ContactMe/ContactMe.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
