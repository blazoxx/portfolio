{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/contact", "destination": "/components/ContactMe/ContactMe.html" },
    { "source": "/coming-soon", "destination": "/components/ComingSoon/ComingSoon.html" },
    { "source": "^(?!/api).*", "destination": "/index.html" }
  ]
}
